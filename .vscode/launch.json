{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}/unittests/frontend-test",
        "name": "Debug HttpService.test.ts",
        "program": "${workspaceFolder}/unittests/frontend-test/node_modules/vitest/vitest.mjs",
        "args": [
            "run",
            "${file}",
            "--testNamePattern=create"
        ],
        "autoAttachChildProcesses": true,
        "smartStep": true,
        "console": "integratedTerminal"
      }
    ]
  }
  
            

